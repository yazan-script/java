package javaapplication11;
import java.util.Scanner;
public class JavaApplication11 {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        System.out.print("Enter number of star : ");
        int n = input.nextInt();

        for (int i = 0; i < n; i++) {
            System.out.print("*");
        }
      
    
        
    }
    
}
