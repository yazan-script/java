package javaapplication11;
import java.util.Scanner;
public class JavaApplication11 {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        System.out.print("أدخل حجم المربع: ");
        int n = input.nextInt();

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                System.out.print("*");
            }
            System.out.println();
        }        
    }
    
}
